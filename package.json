{
  "name": "github-flow",
  "version": "1.0.5",
  "private": true,
  "type": "module",
  "description": "Github/gitlab flow command line tool.",
  "license": "MIT",
  "scripts": {
    "start": "go run main.go",
    "build": "go build -ldflags '-s -w' -o dist/gfl main.go",
    "bin:link": "ln -sf $(pwd)/dist/gfl /usr/local/bin/gfl",
    "bin:unlink": "rm /usr/local/bin/gfl",
    "prepare": "husky",
    "release": "release-it"
  },
  "dependencies": {
    "husky": "^9.1.6"
  },
  "keywords": [
    "flow",
    "gfl",
    "git",
    "gitflow",
    "github",
    "cli"
  ]
}
