{
  "name": "github-flow",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "My github flow.",
  "license": "MIT",
  "scripts": {
    "start": "go run main.go",
    "build": "go build -ldflags '-s -w' -o dist/gfl main.go",
    "bin:link": "ln -s $(pwd)/dist/gfl /usr/local/bin/gfl",
    "bin:unlink": "rm /usr/local/bin/gfl",
    "prepare": "husky"
  },
  "dependencies": {
    "husky": "^9.1.6"
  }
}
